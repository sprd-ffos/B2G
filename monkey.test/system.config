#normal test config define, maybe used in more than one script
ADB=$(which adb)
[ $? -eq 0 ] || ADB=bin/adb
FASTBOOT=bin/fastboot
BUSYBOX=/data/busybox
GSNAP=/data/gsnap
FIND="$ADB shell $BUSYBOX find"
DEVDIR=/data
SNAPSHOT_NAME=snapshot
LASTLOGHEAD=mtlog-last
NOWLOGHEAD=mtlog-now
LOGBACKUP=log_backup

error_test()
{
    if [ "$1" -ne 0 ]
    then
        echo "[ERROR] $2($3)"
        exit $1
    fi
}

