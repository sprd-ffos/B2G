#[Image.config]
export MTCFG_IMG_FOLDER=images
export MTCFG_IMG_GECKOSYMBOLS=${MTCFG_IMG_FOLDER}/objdir-gecko/dist/crashreporter-symbols

#We support "img get" by the following method:
#  * URL - a tar.bz2 file url
#  * SSHFS - give id&passwd for a sshfs to get a tar.bz2
#  * FS - a local tar.bz2 file
#  * NONE - images already prepared
export MTCFG_IMG_GET=URL

#We only need config one of the following kindred image configs.
#[Img.config.url]
export MTCFG_IMG_URL="http://10.0.0.217:8080/hudson/job/sp6821a_gonk4.0/lastSuccessfulBuild/artifact/sp6821a_gonk4.0.tar.gz"

